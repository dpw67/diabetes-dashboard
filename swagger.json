{
  "swagger" : "2.0",
  "info" : {
    "version" : "0.0.0",
    "title" : "Diabetes Dashboard API",
    "description" : "API for dashboard with my diabetes data",
    "contact" : {
      "name" : "Doug Warren",
      "url" : "http://wordpress.dpw67.com/support",
      "email" : "support@dpw67.oib.com"
    },
    "termsOfService" : "http://wordpress.dpw67.com/terms",
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "produces" : [ "application/json" ],
  "host" : "localhost:3000",
  "basePath" : "/api",
  "paths" : {
    "/info" : {
      "get" : {
        "tags" : [ "info" ],
        "summary" : "returns basic info of the server",
        "responses" : {
          "200" : {
            "description" : "successful operation"
          }
        }
      }
    },
    "/moments" : {
      "get" : {
        "tags" : [ "moment" ],
        "summary" : "returns all moments",
        "responses" : {
          "200" : {
            "description" : "A list of moments.",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/moment"
              }
            }
          }
        }
      }
    },
    "/moments/{id}" : {
      "get" : {
        "tags" : [ "moment" ],
        "description" : "Returns moments based on ID",
        "summary" : "Find moments by ID",
        "operationId" : "getMomentsById",
        "produces" : [ "application/json", "text/html" ],
        "responses" : {
          "200" : {
            "description" : "moment response",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/moment"
              }
            }
          }
        },
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "description" : "ID of moment to use",
          "required" : true,
          "type" : "array",
          "items" : {
            "type" : "string"
          },
          "collectionFormat" : "csv"
        } ]
      },
      "put" : {
        "tags" : [ "moment" ],
        "summary" : "saves a specific moment with the identified id",
        "responses" : {
          "200" : {
            "description" : "A moment.",
            "schema" : {
              "type" : "object",
              "items" : {
                "$ref" : "#/definitions/moment"
              }
            }
          }
        }
      },
      "post" : {
        "tags" : [ "moment" ],
        "summary" : "saves a specific moment with the identified id",
        "responses" : {
          "200" : {
            "description" : "A moment.",
            "schema" : {
              "type" : "object",
              "items" : {
                "$ref" : "#/definitions/moment"
              }
            }
          }
        }
      },
      "delete" : {
        "tags" : [ "moment" ],
        "summary" : "deletes a specific moment with the identified id",
        "responses" : {
          "200" : {
            "description" : "A moment.",
            "schema" : {
              "type" : "object",
              "items" : {
                "$ref" : "#/definitions/moment"
              }
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "moment" : {
      "type" : "object",
      "required" : [ "type", "time", "value" ],
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "type" : {
          "type" : "string"
        },
        "time" : {
          "type" : "string",
          "format" : "date-time"
        },
        "value" : {
          "type" : "string",
          "format" : "double"
        }
      }
    }
  }
}